namespace rodash
  ' Gets the milliseconds.
  ' @since 0.0.21
  ' @category Date
  ' @returns {Object} - The milliseconds.
  function getMilliseconds() as object
    dateObj = rodash.internal.getDateObject()

    return {
      "utc": dateObj.utc.getMilliseconds(),
      "local": dateObj.local.getMilliseconds()
    }
  end function
end namespace
