namespace rodash
  ' Gets the year.
  ' @since 0.0.21
  ' @category Date
  ' @returns {Object} - The year.
  function getYear() as object
    dateObj = rodash.internal.getDateObject()

    return {
      "utc": dateObj.utc.getYear(),
      "local": dateObj.local.getYear()
    }
  end function
end namespace
