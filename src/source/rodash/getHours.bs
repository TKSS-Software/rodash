namespace rodash
  ' Gets the hours.
  ' @since 0.0.21
  ' @category Date
  ' @param {Boolean} asLocal - Whether to get the local hours. Default is false.
  ' @returns {Object} The hours.
  @inline
  function getHours(asLocal = false as Boolean) as Object
    return rodash.internal.getDateObject(asLocal).getHours()
  end function
end namespace
