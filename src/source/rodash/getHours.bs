namespace rodash
  ' * Gets the hours.
  ' * @since 0.0.21
  ' * @category Date
  ' * @returns {Object} - The hours.
  function getHours() as object
    dateObj = rodash.internal.getDateObject()

    return {
      "utc": dateObj.utc.getHours(),
      "local": dateObj.local.getHours()
    }
  end function
end namespace
