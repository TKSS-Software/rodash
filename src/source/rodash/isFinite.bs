import "internal/getConstants.bs"
import "gt.bs"
import "isAA.bs"
import "isNumber.bs"
import "lt.bs"

namespace _
  function isFinite(value as dynamic) as boolean
    if NOT _.isNumber(value) then return false
    constants = _.internal.getConstants()

    if _.gt(value, constants.max_int) OR _.lt(value, constants().min_int) then return false

    return true
  end function
end namespace