namespace rodash
  ' Gets the month.
  ' @since 0.0.21
  ' @category Date
  ' @returns {Object} The month.
  function getMonth() as Object
    dateObj = rodash.internal.getDateObject()

    return {
      "utc": dateObj.utc.getMonth(),
      "local": dateObj.local.getMonth()
    }
  end function
end namespace
