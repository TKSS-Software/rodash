namespace rodash
  ' Converts a string to lower case.
  ' @since 0.0.21
  ' @category String
  function lowerCase(value = "" as string)
    value = value.replace("-", " ").replace("_", " ")
    value = value.trim()
    valueArray = value.split(" ")
    return lcase(rodash.join(valueArray, " "))
  end function
end namespace
