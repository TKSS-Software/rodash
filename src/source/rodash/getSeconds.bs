namespace rodash
  ' * @since 0.0.21
  ' * @category Date
  ' * Gets the seconds.
  ' * @returns {Object} - The seconds.
  function getSeconds() as object
    dateObj = rodash.internal.getDateObject()

    return {
      "utc": dateObj.utc.getSeconds(),
      "local": dateObj.local.getSeconds()
    }
  end function
end namespace
