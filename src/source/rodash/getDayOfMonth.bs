namespace rodash
  ' * Gets the day of the month.
  ' * @since 0.0.21
  ' * @category Date
  ' * @returns {Object} - The day of the month.
  function getDayOfMonth() as object
    dateObj = rodash.internal.getDateObject()

    return {
      "utc": dateObj.utc.getDayOfMonth(),
      "local": dateObj.local.getDayOfMonth()
    }
  end function
end namespace
