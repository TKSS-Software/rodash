namespace rodash
  ' Gets the day of the week.
  ' @since 0.0.21
  ' @category Date
  ' @returns {Object} - The day of the week.
  function getDayOfWeek() as object
    dateObj = rodash.internal.getDateObject()

    return {
      "utc": dateObj.utc.getDayOfWeek(),
      "local": dateObj.local.getDayOfWeek()
    }
  end function
end namespace
