namespace rodash
  ' Gets the day of the week.
  ' @since 0.0.21
  ' @category Date
  ' @param {Boolean} asLocal - Whether to get the local day of the week. Default is false.
  ' @returns {Object} The day of the week.
  @inline
  function getDayOfWeek(asLocal = false as Boolean) as Object
    return rodash.internal.getDateObject(asLocal).getDayOfWeek()
  end function
end namespace
