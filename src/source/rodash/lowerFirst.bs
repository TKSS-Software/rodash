namespace rodash
  ' * Converts the first character of string to lower case.
  ' * @since 0.0.21
  ' * @category String
  function lowerFirst(value = "" as string)
    value = value.trim()
    valueArray = value.split("")
    valueArray[0] = lcase(valueArray[0])
    return rodash.join(valueArray)
  end function
end namespace

