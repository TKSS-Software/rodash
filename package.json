{
  "name": "@tkss/rodash",
  "version": "0.0.21",
  "description": "A lodash inspired utility for Roku development",
  "main": "index.js",
  "keywords": [
    "ropm",
    "roku"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TKSS-Software/rodash.git"
  },
  "scripts": {
    "preversion": "npm run build",
    "build": "npm run compile && ts-node ./scripts/createDist.ts",
    "compile": "npx bsc",
    "docs": "npm run build && jsdoc -c jsdoc.json -d docs -t ./node_modules/braintree-jsdoc-template",
    "build-test-project": "cd test-project && npx bsc"
  },
  "author": "TKSS Software",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "ropm": {
    "packageRootDir": "dist"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.7.16",
    "braintree-jsdoc-template": "^3.3.0",
    "brighterscript": "^0.67.5",
    "brighterscript-jsdocs-plugin": "^0.6.0",
    "concat": "^1.0.3",
    "copyfiles": "^2.4.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "merge-files": "^0.1.2",
    "replace-in-files": "^3.0.0",
    "rooibos-cli": "^1.4.0",
    "rooibos-roku": "^5.12.0",
    "ropm": "^0.10.25",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
