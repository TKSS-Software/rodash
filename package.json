{
  "name": "@tkss/rodash",
  "version": "0.0.11",
  "description": "A lodash inspired utility for Roku development",
  "main": "index.js",
  "keywords": [
    "ropm",
    "roku"
  ],
  "repository": {
    "type": "git",
