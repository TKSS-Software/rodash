{
	"rootDir": "./",
	"autoImportComponentScript": true,
	"files": [
		"components/*.+(xml|brs|bs)",
		"source/*.+(xml|brs|bs)",
		"assets/**/*",
		"manifest"
	],
	"plugins": [
		"rooibos-roku"
	],
	"rooibos": {
		"showOnlyFailures": true,
		"lineWidth": 70,
		"logLevel": 0
	},
	"emitDefinitions": true
}
